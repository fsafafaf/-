.page__hd{
  width: 100%;
  height: 356rpx;
  position: relative;
}
.bg{
  width: 100%;
  height: 356rpx;
  z-index: 1;
}
.bg image {
  width: 100%;
  height: 356rpx;
  z-index: 1;
}
.header {
    display: flex;
    height: 85rpx;
    width: 90%;
    flex-direction: row ;
    justify-content: space-between;
    float: left;
    position: absolute;
    left: 40rpx;
    top: 60rpx;
}
.page__hd_yuan{
  display: flex;
  width: 70rpx;
  height: 70rpx;
  border-radius: 60rpx;
  background: rgba(111,111,111,.4);
}
.page__hd_bd{
  width: 400rpx;
  height: 70rpx;
  background: rgba(111,111,111,.4);
  border-radius: 40rpx;
  font-size: 40rpx;
  color: #ffffff;
  text-align: center;

}
.page__hd_ft image{
  width: 60rpx;
  height: 60rpx;
}
.page__bd {
  width:100%;
  height:350rpx;
  background: #ffffff;
}
.page__bd .items{
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
}
.page__bd .kit_items__item{
  display: flex;
  flex-direction: column;
  padding-left: 50rpx;
  flex: 1;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
}
.page__bd .kit_items__item.item-one{
  padding-left: 20rpx;
}
.page__bd image {
  width: 120rpx;
  height: 100rpx;
}
.page__bd .round_btn {
  font-size:25rpx;
  color: #706f6f;
  margin-left: 20rpx;
}
